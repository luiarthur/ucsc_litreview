#!/bin/bash

### SIMULATION ON LOGIN / INTERACTIVE NODE OF SERVER
K="-1 0 1"
SIMS=`seq 1 5`

### SIMS
date
for k in $K
do
  for i in $SIMS # simulation 0 is a small test simulation
  do
    echo "i:$i, k:$k"
    Rscript test_cytof_fix_K_simdat.R $i $k &
  done
done
wait
date

#Rscript test_cytof_fix_K_simdat.R 3 -1
